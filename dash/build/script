NAME="dash"
TAG="0.5.11"
SRC="https://git.kernel.org/pub/scm/utils/dash/dash.git"
_git_clone_tag
_chdir_build

./autogen.sh
./configure --without-libedit
make CFLAGS="-DNDEBUG -Os msse4.2 -pipe -fomit-frame-pointer -ffunction-sections -fdata-sections" LDFLAGS="-static,-Wl,--as-needed,--gc-sections,--strip-all"
mv "src/${NAME}" "${_TARGET}/${NAME}.${TAG}"
